{
  "name": "@mintlify/prebuild",
  "type": "module",
  "version": "1.0.33",
  "description": "Helpful functions for Mintlify's prebuild step",
  "author": "Mintlify, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint",
    "directory": "packages/mintlify-prebuild"
  },
  "bugs": {
    "url": "https://github.com/mintlify/mint/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "prebuild"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc --project tsconfig.build.json --watch",
    "lint": "eslint . --cache",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check"
  },
  "peerDependencies": {
    "@apidevtools/swagger-parser": "10.x",
    "@mintlify/models": ">= 0.0.15 < 1",
    "@mintlify/validation": ">= 0.1.57 < 1",
    "fs-extra": "11.x",
    "gray-matter": "^4.0.3",
    "openapi-types": "12.x",
    "unist-util-visit": "^4.1.1"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "resolver": "ts-jest-resolver"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/models": "0.0.29",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "1.0.7",
    "@mintlify/validation": "0.1.63",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "cross-env": "^7.0.3",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "jest": "^29.4.3",
    "prettier": "2.x",
    "ts-jest": "^29.0.5",
    "typescript": "^4.8.2"
  },
  "gitHead": "f9a29f3de6300880517b4748f7b4822c625680c2"
}
