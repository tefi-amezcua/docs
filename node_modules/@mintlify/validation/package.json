{
  "name": "@mintlify/validation",
  "version": "0.1.63",
  "description": "Validates mint.json files",
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "validation"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "resolver": "ts-jest-resolver",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ]
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "test:types": "tsc --noEmit",
    "test:jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test": "npm run test:types && npm run test:jest",
    "lint": "eslint . --cache",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check"
  },
  "dependencies": {
    "lcm": "^0.0.3",
    "zod": "^3.20.6",
    "zod-to-json-schema": "^3.20.3"
  },
  "peerDependencies": {
    "@mintlify/models": "0.0.25",
    "openapi-types": "12.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/models": "0.0.29",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "1.0.7",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/lcm": "^0.0.0",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "babel-jest": "^29.3.1",
    "cross-env": "^7.0.3",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "jest": "^29.3.1",
    "openapi-types": "^12.1.0",
    "prettier": "2.x",
    "ts-expect": "^1.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.8.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "gitHead": "f9a29f3de6300880517b4748f7b4822c625680c2"
}
