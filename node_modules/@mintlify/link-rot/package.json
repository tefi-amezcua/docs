{
  "name": "@mintlify/link-rot",
  "version": "3.0.33",
  "description": "Static checking for broken internal links",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "link",
    "rot"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "watch": "tsc --watch",
    "lint": "eslint . --cache",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check"
  },
  "jest": {
    "preset": "ts-jest",
    "resolver": "ts-jest-resolver",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(is-absolute-url)/)"
    ]
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.x",
    "@mintlify/prebuild": "1.0.33",
    "chalk": "^5.1.0",
    "fs-extra": "^11.1.0",
    "is-absolute-url": "^4.0.1",
    "openapi-types": "^12.0.0",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx": "^2.2.1",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "1.0.7",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.2",
    "@types/mdast": "^3.0.10",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "jest": "^29.5.0",
    "mdast-util-mdx-jsx": "^2.1.2",
    "mock-fs": "^5.2.0",
    "prettier": "2.x",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^4.8.2"
  },
  "gitHead": "f9a29f3de6300880517b4748f7b4822c625680c2"
}
